void setup()

{

  size(250, 250);

}

void draw()

{

  fill(0, 0, 255);      //blue rectangles

  rect(5, 5, 25, 25);

  rect(55, 55, 25, 25);

  

  fill(0, 255, 0);      //green rectangles

  rect(30, 30, 25, 25);

  rect(105, 105, 25, 25);

  fill(255, 0, 0);      //red rectangles

  rect(80, 80, 25, 25);

  rect(130, 130, 25,25);

}


